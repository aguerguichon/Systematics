#processList=["WWlvlv", "WZlvll", "ZZ4l", "ZZvvll", "Ztautau", "ttbar"]
processList=["WWlvlv", "WZlvll"]
isScaledList=["", "_scaled"]
os.system("cd /sps/atlas/a/aguerguichon/Calibration/DataxAOD/testMerge/")

for iProcess in processList:
    for isScaled in isScaledList:
        directoryName= "MC_13TeV_"+iProcess+"_2015c_Lkh1"+isScaled
        os.system("cd "+directoryName)
        os.system("pwd")
        print "toto"
        result= sub.check_output(["ls OK*"])
        
        print "PRINT: "+result
        versionNumber=result.split("OK")[1]
        os.system("cd "+sub.check_output("ls user.aguergui*", shell=True))
        os.system("AddFiles --infile user.aguergui* --outName /sps/atlas/a/aguerguichon/Calibration/DataxAOD/testMerge/"+directoryName+"/"+directoryName+"_"+versionNumber+".root")
        os.system("cd ..")
